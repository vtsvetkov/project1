<div id="idds_wrapper">
  <% @project.idds.each do |idd| %>
  <%= link_to idd.integration_id, idd_path(idd) %>
  <%= link_to "Delete IDD", project_idd_path(@project, idd), method: :delete, data: { confirm: "Are you sure?"} %>
  <br>
<% end %>
</div>

<%= link_to 'Create New IDD', new_project_idd_path(@project) %>
